<?xml version="1.0" encoding="utf-8"?>
<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/checked"
        android:drawable="@drawable/miuix_appcompat_btn_checkbox_on"
        android:state_checked="true" />
    <item
        android:id="@+id/unchecked"
        android:drawable="@drawable/miuix_appcompat_btn_checkbox_off"
        android:state_checked="false" />

    <transition
        android:drawable="@drawable/miuix_appcompat_btn_checkbox_off_to_on"
        android:fromId="@id/unchecked"
        android:toId="@id/checked" />
    <transition
        android:drawable="@drawable/miuix_appcompat_btn_checkbox_on_to_off"
        android:fromId="@id/checked"
        android:toId="@id/unchecked" />
</animated-selector>
